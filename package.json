{
  "name": "nestjs-email-service",
  "version": "0.0.1",
  "description": "NestJs email module",
  "keywords": [
    "nestjs",
    "email"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rm -rf dist && rm *.tgz",
    "build": "tsc -p tsconfig.json",
    "postbuild": "npm pack && mv nestjs-email-service-*.tgz compiled-lib.tgz",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest"
  },
  "author": "Vong Panha Huot",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^7.5.2",
    "@nestjs/core": "^7.5.2",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.0",
    "handlebars": "^4.7.6",
    "mjml": "^4.7.1"
  }
}
